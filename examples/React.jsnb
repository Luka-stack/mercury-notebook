{"chapters":[{"id":"defaultChapter","description":{"id":"chapterDescription","type":"text","content":"# React Example","chapterId":"defaultChapter"},"content":["vzl","7ex","zjb","4wj","k5e","9e2"]}],"cells":[{"id":"vzl","type":"text","content":"### Imports ","chapterId":"defaultChapter"},{"id":"7ex","type":"code","content":"import React, { useState } from 'react';\nimport ReactDOM from 'react-dom';","chapterId":"defaultChapter"},{"id":"zjb","type":"text","content":"### Components","chapterId":"defaultChapter"},{"id":"4wj","type":"code","content":"const Button = ({ text, onClick }) => {\n  return <button onClick={onClick}>{text}</button>;\n};\n\nconst Header = ({ text }) => {\n  const [selected, setSelected] = useState(false);\n\n  const clicked = () => {\n    setSelected(!selected);\n  };\n\n  return (\n    <div\n      onClick={clicked}\n      style={{ backgroundColor: selected ? 'tomato' : 'blue', color: 'white' }}\n    >\n      <h1>{text}</h1>\n    </div>\n  );\n};","chapterId":"defaultChapter"},{"id":"k5e","type":"text","content":"### App","chapterId":"defaultChapter"},{"id":"9e2","type":"code","content":"const App = () => {\n    const [number, setNumber] = useState(0);\n\n    const increment = () => {\n      setNumber(number + 1)\n    }\n\n    const decrement = () => {\n      setNumber(number - 1);\n    }\n\n    return (\n    <div style={{ display: \"flex\", flexDirection: \"column\" }}>\n      <Header text={\"Incrementing Test\"} />\n\n      <br />\n      <p>Clicked {number} times</p>\n      <br />\n\n      <Button text={'Increment'} onClick={increment} />\n      <Button text={'Decrement'} onClick={decrement} />\n    </div>\n  );\n};\n\nReactDOM.render(<App />, document.querySelector('#root'));","chapterId":"defaultChapter"}]}